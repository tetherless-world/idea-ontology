    <!-- http://purl.org/ontology/bibo/Event -->

    <owl:Class rdf:about="http://purl.org/ontology/bibo/Event">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:subClassOf>
            <owl:Class>
