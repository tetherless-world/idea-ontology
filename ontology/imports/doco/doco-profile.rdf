    <!-- http://purl.org/spar/doco/Abstract -->

    <owl:Class rdf:about="http://purl.org/spar/doco/Abstract">
        <rdfs:comment>A brief summary of a book, a research article, thesis, review, conference proceeding or any in-depth analysis of a particular subject or discipline, the purpose of which is to help the reader quickly ascertain the publication&apos;s purpose.</rdfs:comment>
        <rdfs:label>abstract</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/doco/ChapterTitle -->

    <owl:Class rdf:about="http://purl.org/spar/doco/ChapterTitle">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/doco/Title"/>
        <owl:disjointWith rdf:resource="http://purl.org/spar/doco/SectionTitle"/>
        <rdfs:comment xml:lang="en">The title of a chapter.</rdfs:comment>
        <rdfs:label xml:lang="en">chapter title</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/doco/Figure -->

    <owl:Class rdf:about="http://purl.org/spar/doco/Figure">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:comment xml:lang="en">A communication object comprising one or more graphics, drawings, images, or other visual representations..</rdfs:comment>
        <rdfs:label xml:lang="en">figure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/doco/List -->

