    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasEmbargoDuration -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasEmbargoDuration">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
        <rdfs:comment xml:lang="en">The time period for which an entity is embargoed.  During this period, the entity should not be published or, in the case of a press release, should not be reported on.  For open-access journal articles, the embargo duration specifies that period of time during which availability of the open-access version of the article is delayed by the publisher, following subscription-access availability of the published work.  The end of the embargo period can be specified by fabio:hasEmbargoDate.</rdfs:comment>
        <rdfs:label xml:lang="en">has embargo period</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasHandle -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasHandle">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A persistent identifier of the Handel system for digital objects and other resources on the Internet.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.handle.net/"/>
        <rdfs:label xml:lang="en">has handle</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasIssnL -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasIssnL">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdfs:comment xml:lang="en">A linking International Standard Serial Number.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.issn.org/2-22637-What-is-an-ISSN-L.php"/>
        <rdfs:label xml:lang="en">has ISSN-L</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasNLMJournalTitleAbbreviation -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasNLMJournalTitleAbbreviation">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/spar/fabio/hasShortTitle"/>
        <rdfs:comment xml:lang="en">An internal identifier for the abbreviation of the title of journals available from the National Library of Medicine repository.</rdfs:comment>
        <rdfs:label xml:lang="en">has National Library of Medicine journal title abbreviation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasNationalLibraryOfMedicineJournalId -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasNationalLibraryOfMedicineJournalId">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>[0-9]+</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en">An internal identifier for journals available from the National Library of Medicine repository.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.nlm.nih.gov/"/>
        <rdfs:label xml:lang="en">has National Library of Medicine journal identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasPII -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasPII">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdfs:comment xml:lang="en">Has Publisher Item Identifier</rdfs:comment>
        <rdfs:label xml:lang="en">has PII</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://en.wikipedia.org/wiki/Publisher_Item_Identifier"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasPageCount -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasPageCount">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
        <rdfs:comment xml:lang="en">The count of the number of pages in a textual resource.</rdfs:comment>
        <rdfs:label xml:lang="en">has page count</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasPatentNumber -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasPatentNumber">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdfs:comment xml:lang="en">A unique identifing number issued by a patent authority to identify a patent, displayed at the beginning of the patent document.</rdfs:comment>
        <rdfs:label xml:lang="en">has patent number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasPubMedCentralId -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasPubMedCentralId">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">An identifier for bibliographic entities hosted by the PubMed Central repository.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ncbi.nlm.nih.gov/pmc/"/>
        <rdfs:label xml:lang="en">has PubMed Central identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasPubMedId -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasPubMedId">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">An identifier for bibliographic records held by the PubMed repository.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ncbi.nlm.nih.gov/pubmed/"/>
        <rdfs:label xml:lang="en">has PubMed identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasPublicationYear -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasPublicationYear">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/issued"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
        <rdfs:comment xml:lang="en">The year in which a resource is published.</rdfs:comment>
        <rdfs:label xml:lang="en">has publication year</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasRequestDate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasRequestDate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
        <rdfs:comment xml:lang="en">The date on which an agent is requested to do something, for example a reviewer is requested to write a review of a paper submitted to a journal for publication, or an author is requested to supply a revised version of the paper in response to the reviews received.</rdfs:comment>
        <rdfs:label xml:lang="en">has request date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasRetractionDate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasRetractionDate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">The date on which something, for example a claim or a journal article, is retracted.</rdfs:comment>
        <rdfs:label xml:lang="en">has retraction date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasSICI -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasSICI">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The Serial Item and Contribution Identifier is a code used to uniquely identify specific volumes, articles or other identifiable parts of a periodical. It is intended primarily for use by those members of the bibliographic community involved in the use or management of serial titles and their contributions.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.niso.org/apps/group_public/project/details.php?project_id=75"/>
        <rdfs:label xml:lang="en">has SICI</rdfs:label>
        <rdfs:label xml:lang="en">has Serial Item and Contribution Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasSeason -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasSeason">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment xml:lang="en">Permits specification of the season of the year, for example spring, summer, autumn and winter in British English.</rdfs:comment>
        <rdfs:label xml:lang="en">has season</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasSequenceIdentifier -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasSequenceIdentifier">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment xml:lang="en">A literal (for example a number or a letter) that identifies the sequence position of a work within a particular context, for example a book in a book series, a chapter in a document, a volume in a journal.</rdfs:comment>
        <rdfs:label xml:lang="en">has number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasShortTitle -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasShortTitle">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/title"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A short version of the title of an entity, typically used to label or refer to a particular entity in an abbreviated form, for example an abbreviated journal title in a reference, or a short title of a document used as the running title in a page header.</rdfs:comment>
        <rdfs:label xml:lang="en">has short title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasStandardNumber -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasStandardNumber">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdfs:comment xml:lang="en">The number defining an international standard, for example Z39.96 - 201x, identifying NISO JATS, the Journal Article Tag Suite.</rdfs:comment>
        <rdfs:label xml:lang="en">has standard number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasSubtitle -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasSubtitle">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/title"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A secondary title that follows the main title of a work.</rdfs:comment>
        <rdfs:label xml:lang="en">has subtitle</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasTranslatedSubtitle -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasTranslatedSubtitle">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/title"/>
        <rdfs:comment xml:lang="en">A version of the subtitle of an entity translated into another language, which may be specified using the object property dcterms:language.</rdfs:comment>
        <rdfs:label xml:lang="en">has translated subtitle</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasTranslatedTitle -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasTranslatedTitle">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/title"/>
        <rdfs:comment xml:lang="en">A version of the title of an entity translated into another language, which may be specified using the object property dcterms:language.</rdfs:comment>
        <rdfs:label xml:lang="en">has translated title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasURL -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasURL">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">An identifier, in form of an HTTP Universal Resource Locator (URL), for a particular resource on the World Wide Web.</rdfs:comment>
        <rdfs:label xml:lang="en">has URL</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/hasVolumeCount -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/hasVolumeCount">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment xml:lang="en">The count of the number of volumes a work includes.</rdfs:comment>
        <rdfs:label xml:lang="en">has volume count</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/fabio/usesCalendar -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/fabio/usesCalendar">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="en">A property that identifies the calendar system used to specify a date, for example the Chinese, Gregorian, Hebrew, Islamic or Lunar calendar.</rdfs:comment>
        <rdfs:label xml:lang="en">uses calendar</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topDataProperty -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2002/07/owl#topDataProperty"/>
    


